{
  "compilerOptions": {
    "target": "es5",
    "baseUrl": "./src",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "plugins": [{ "name": "typescript-plugin-css-modules" }],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "typedocOptions": {
    "entryPoints": [
      "./src/index.tsx",
      "./src/Editor/Components",
      "./src/Editor/Feature",
      "./src/Editor/Adapters",
      "./src/Editor/Components/Utilities",
      "./src/Editor/Components/ContBlocks",
      "./src/Editor/Components/PetriNets",
      "./src/Editor/Model/UtilClasses",
      "./src/Editor/Model/ContBlocks",
      "./src/Editor/Model/PetriNets",
      "./src/App.tsx"
    
    ],
    "includes": "./src",
    "out": "./docs/Editor"
 },
  "include": [
    "src"
  ]
}
